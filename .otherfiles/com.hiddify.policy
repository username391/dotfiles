<?xml version="1.0" encoding="UTF-8"?>
<policyconfig>
  <action id="com.hiddify.start">
    <description>Запустить Hiddify от имени администратора</description>
    <message>Введите пароль для запуска Hiddify</message>
    <defaults>
      <allow_any>auth_admin</allow_any>
      <allow_inactive>auth_admin</allow_inactive>
      <allow_active>auth_admin</allow_active>
    </defaults>
  </action>
</policyconfig>

